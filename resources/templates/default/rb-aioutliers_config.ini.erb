[OutliersServerProduction]
outliers_binding_address=0.0.0.0
outliers_server_port=39091
outliers_server_workers=4
outliers_server_threads=20

[OutliersServerTesting]
outliers_binding_address=0.0.0.0
outliers_server_port=39091

[Outliers]
metric=bytes
epochs=1000
batch_size=32
backup_path=resources/src/ai/backups/

[ShallowOutliers]
sensitivity=<%=@sensitivity%>
contamination=<%=@contamination%>

[Druid]
druid_endpoint=http://<%=@druid_broker%>/druid/v2/

[Logger]
log_file=<%=@log_file%>

[Rails]
endpoint=https://<%=@rails_host%>
auth_token=<%=@rails_token%>

[AWS]
s3_hostname=http://<%=@s3_host%>:<%=@s3_port%>
s3_public_key=<%=@s3_access_key%>
s3_private_key=<%=@s3_secret_key%>
s3_region=es
s3_bucket=<%=@s3_bucket%>

[ZooKeeper]
zk_sleep_time=86400
zk_tick_time=5
zk_hosts=<%=@zookeeper_hosts%>
zk_sync_path=/rb-aioutliers
zk_name=<%=@zk_name%>
